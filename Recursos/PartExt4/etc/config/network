config interface 'vpn'
  option proto 'wireguard'
  option private_key 'xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx='
  option listen_port '51820'
  list addresses '192.168.255.1/24'
  option force_link '1'
  list dns '192.168.255.1'
  option delegate '0'
